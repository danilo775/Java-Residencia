package calculadora;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite um numeero");
		int a = sc.nextInt();
		System.out.println("Digite um numeero");
		int b = sc.nextInt();
		
		Calculadora.dividir(a, b);
		
	}
}
